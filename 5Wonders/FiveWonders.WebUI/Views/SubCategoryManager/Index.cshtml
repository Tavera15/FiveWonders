@model IEnumerable<FiveWonders.core.Models.SubCategory>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.mTimeEntered)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.mSubCategoryName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.isEventOrTheme)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.mTimeEntered)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.mSubCategoryName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.isEventOrTheme)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.mID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.mID })
        </td>
    </tr>
}

</table>

<div>
    @Html.ActionLink("Back to Managers", "Index", "Managers")
</div>