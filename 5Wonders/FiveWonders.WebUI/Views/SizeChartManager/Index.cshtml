@model IEnumerable<FiveWonders.core.Models.SizeChart>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.mID)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.mTimeEntered)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.mChartName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.mImageChartUrl)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.mID)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.mTimeEntered)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.mChartName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.mImageChartUrl)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id = item.mID }) |
            @Html.ActionLink("Delete", "Delete", new { id = item.mID })
        </td>
    </tr>
}

</table>

<div>
    @Html.ActionLink("Back to Managers", "Index", "Managers")
</div>