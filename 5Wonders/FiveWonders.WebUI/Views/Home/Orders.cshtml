@model IEnumerable<FiveWonders.core.Models.FWonderOrder>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.mTimeEntered)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.mCustomerName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.mCustomerEmail)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.mAddress1)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.mAddress2)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.mZipCode)
