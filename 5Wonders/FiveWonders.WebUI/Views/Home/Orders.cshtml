            @Html.DisplayFor(modelItem => item.mZipCode)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.mPhoneNumber)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.paypalRef)
        </td>

        <td></td>
    </tr>
