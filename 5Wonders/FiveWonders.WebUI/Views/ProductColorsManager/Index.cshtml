@model IEnumerable<FiveWonders.core.Models.CustomOptionList>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.mName)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.mName)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new {  id=item.mID  }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.mID  })
        </td>
    </tr>
}

</table>
