@model IEnumerable<FiveWonders.core.Models.FWonderOrder>

<div style="padding: 2rem;">

    <table class="table">
        <tr>
            <th>
                Order Submission
            </th>
            <th>
                Order ID
            </th>
            <th></th>
        </tr>

    @foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.mTimeEntered)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.mID)
            </td>
            <td>
                @Html.ActionLink("View Order Details", "Details", new { id=item.mID, VerificationId = item.mVerificationId })
            </td>
        </tr>
    }

    </table>

</div>