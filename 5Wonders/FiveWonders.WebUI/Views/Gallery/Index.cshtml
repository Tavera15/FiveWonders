@model IEnumerable<FiveWonders.core.Models.InstagramPost>

<div class="container" style="margin-top: 5px; padding: 0; background-color: whitesmoke">
    @foreach (var item in Model) {
        <img src="@item.mImageURL" class="col-lg-4 col-md-4 col-sm-6 col-xs-12" style="height: 40vh; margin: 0; padding: 5px;" alt="@item.mImageURL"/>
    }
</div>

